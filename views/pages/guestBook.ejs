<%- include('../shared/header.ejs') %>

<table class="table">
<thead class="thead-dark">
<tr>
    <th>ID</th>
    <th>Username</th>
    <th>Country</th>
    <th>Date</th>
    <th>Message</th>
  </tr>
  </thead>
    <% for (var i = 0; i < data.length; i++) { %>
    <tr>
        <td><%= data[i].id %></td>
        <td><%= data[i].username %></td>
        <td><%= data[i].country %></td>
        <td><%= data[i].date %></td>
        <td><%= data[i].message %></td>
    </tr>
       
    <% } %>
</table>

<%- include('../shared/footer.ejs') %>